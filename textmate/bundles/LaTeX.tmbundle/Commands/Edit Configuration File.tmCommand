    cat "$TM_BUNDLE_SUPPORT/config/latex.config" &gt; "$AccountFile"
fi
open txmt://open?url=file://$AccountFile</string>
	<key>input</key>
	<string>none</string>
	<key>name</key>
	<string>Edit Configuration File</string>
	<key>output</key>
	<string>discard</string>
	<key>scope</key>
	<string>text.tex.latex</string>
	<key>uuid</key>
	<string>D10F1CAC-BF1D-456D-B1E5-6AA5E3DD40ED</string>
</dict>
